        <script><![CDATA[function onSubmit() {

	var projectTitle = g_form.getValue("u_project_title");
	var date = g_form.getValue("u_requested_draft");
	var due = g_form.getValue("u_assets_due");	
	
	if (projectTitle == 'Please fix draft review date') {
		
		alert("Please ensure requested date is 14 days or later since your Priority is 2 or higher");
		return false;
	} else if (!(date <= due)){
		alert("Please ensure the final assets due date is greater than or equal to the requested draft review date");
		return false;
	}
   
}]]></script>
